class CornerRight{
 public float coordX;
 public float coordY;
 public float cornerDeep;
 
 CornerRight(float x, float y)
 {
   coordX = x;
   coordY = y;
 }
 
 CornerRight(float x,float y, float deep)
 {
   cornerDeep = deep;
   coordX = x;
   coordY = y;
 }
}

